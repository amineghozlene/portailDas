<div class="tab" data-animation="slide">
    <ul class="tab-heading">
        <li class="current"><h6><a href="#">Log in</a></h6></li>
        <li><h6><a href="#">Register</a></h6></li>
    </ul><!-- .tab-heading -->
    <div class="tab-content">
        <div class="current">
            <table>
                <tr>
                    <td>immatriculation</td>
                    <td>nom &Aacute; prénom</td>
                    <td>action</td>
                </tr>
                @{
                    foreach (Compte compte in CompteDAO.recupererCompteElearning(((Compte)Session["compteUtilisateur"]).idSociete.idSociete))
                    {
                        <tr>
                            <td>@compte.login</td>
                            <td>@compte.prenom  @compte.nom</td>
                            <td>
                                @if (compte.etatValidation.Equals("nonValidé"))
                                {
                                    <a href="#" onclick="validerUser(@compte.login)">validate</a>
                                }
                                else
                                {
                                    <a href="#" onclick="editUser(@compte.login)">edit</a>
                                }
                                <a href="#" onclick="deleteUser(@compte.login)">delete</a>
                            </td>
                        </tr>
                    }
                }
            </table>
        </div>
    </div>
</div>


