<?xml version="1.0" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="PortailDAS">
  <class name="PortailDAS.Societe" table="Societe" dynamic-update="true">
    <cache usage="read-write"/>
    <id name="idSociete" column="idSociete" type="String" />
    <property name="nomSociete" column="nomSociete" type="string" length="100" />
    <property name="registreDeCommerce" column="registreDeCommerce" type="string" length="100" />
    <property name="matriculeFiscale" column="matriculeFiscale" type="string" length="100" />
    <property name="adresseSociete" column="adresseSociete" type="string" length="100" />

  </class>

</hibernate-mapping>
