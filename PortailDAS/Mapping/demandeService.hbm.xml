<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="PortailDAS">
  <class name="PortailDAS.DemandeService" table="demandeservice">

    <id name="idOrderService" column="idOrderService" type="int" unsaved-value="0">
      <generator class="native" />
    </id>
    <property name="DateOrder" column="dateDemande" type="date" length="100" />
    <property name="nbrOrderService" column="nbrServiceEnDemande" not-null="false" precision="10"  scale="3" type="int" />
    <many-to-one name="idService" column="idService" class="PortailDAS.DemandeService" not-found="ignore" fetch="join" />
    <many-to-one name="idCompte" column="idCompte" class="PortailDAS.DemandeService" not-found="ignore" fetch="join" />

  </class>

</hibernate-mapping>