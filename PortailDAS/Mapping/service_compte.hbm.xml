<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="PortailDAS">
  <class name="PortailDAS.ServiceCompte" table="service_compte" dynamic-update="true">
    <cache usage="read-write"/>
    <id name="idServiceCompte" column="idservice_compte" type="int" />
    <many-to-one name="idSociete" column="idsociete" class="PortailDAS.Societe" cascade="all" not-found="ignore" fetch="join" />
    <many-to-one name="idService" column="idsociete" class="PortailDAS.Service" cascade="all" not-found="ignore" fetch="join" />
    <property name="dateAchat" column="dateAchat" type="string" length="100" />
    <property name="dateExpiration" column="dateExpiration" type="string" length="100" />
  </class>
</hibernate-mapping>